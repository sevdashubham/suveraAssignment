{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-console": [
            "error",
            { "allow": [
                    "info",
                    "error"
                ]
            }
        ]
    }
}